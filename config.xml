<?xml version='1.0' encoding='utf-8'?>
<widget id="com.holostorecard.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>3DHoloStore AR extension</name>
    <description>3D Holostorecard AR extension</description>
    <author email="info@3dholostorecard.com" href="http://3dholostorecard.com">nrgie</author>
    <content src="index.html" />
    <icon src="www/img/logo.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="www/img/logo.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="www/img/logo.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="www/img/logo.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="www/img/logo.png" />
    <icon gap:platform="blackberry" src="www/img/logo.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="www/img/logo.png" />
    <icon gap:platform="ios" height="57" src="www/img/logo.png" width="57" />
    <icon gap:platform="ios" height="72" src="www/img/logo.png" width="72" />
    <icon gap:platform="ios" height="114" src="www/img/logo.png" width="114" />
    <icon gap:platform="ios" height="144" src="www/img/logo.png" width="144" />
    <icon gap:platform="webos" src="www/img/logo.png" />
    <icon gap:platform="winphone" src="www/img/logo.png" />
    <icon gap:platform="winphone" gap:role="background" src="www/img/logo.png" />
    <plugin name="cordova-plugin-inappbrowser" spec="1" />
    <plugin name="cordova-plugin-geolocation" spec="1" />
    <plugin name="com.ezartech.ezar.videooverlay" spec="1" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="EnableWebGL" value="true" />
    <preference name="xwalkVersion" value="19+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
</widget>
